<div>
  <a href="/keys/new">Add</a>
</div>

<% @collection.each do |pair| %>
<div>
  <h3><a href="/keys/<%= pair.id %>"><%= pair.name %></a></h3>
  <INPUT value="<%= pair.public %>" style="width: 100%;"></INPUT>
  <pre><%= pair.private %></pre>
  <button data-href="/keys/<%= pair.id %>" data-method="DELETE" data-sync="yes">Delete</button>
</div>
<% end %>
